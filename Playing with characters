#include<stdio.h>
#include<string.h>
int main(void){
    
    
    char ch;
    char s[100];
    char sen[100];
    
    /* read the input */
    scanf("%c",&ch);
    scanf("%s",s);
    scanf(" %[^\n]",sen);
    
    /*print the input */
    printf("%c\n",ch);
    printf("%s\n",s);
    printf("%s\n",sen);
    return 0;
}
